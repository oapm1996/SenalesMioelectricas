@model SenalesMioelectricas.Models.Paciente

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Detalles</title>
</head>
<body>
    <div>
        <h4>Paciente Detalles</h4>
        <hr />
        <dl class="dl-horizontal">
            <dt>
                @Html.DisplayNameFor(model => model.Nombre)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Nombre)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.Apellido_Paterno)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Apellido_Paterno)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.Apellido_Materno)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Apellido_Materno)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.Sexo)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Sexo)
            </dd>


        </dl>
    </div>
    <p>
        <a href="@Url.Action("Edit", "Paciente", new { id = Model.ID_Paciente })">
            <i class="fa fa-edit btn btn-info btn-sm"></i>
        </a> |
        <a href="@Url.Action("Index", "Paciente")"><i class="btn btn-warning btn-sm">Back</i></a>

    </p>
</body>
</html>
